									</div>
									<div class="card-body">
										<div class="table-responsive">
											<table id="example3" class="display" style="min-width: 845px">
												<thead>
													<tr>
														<th>#</th>
														<th>First Name</th>
														<th>Last Name</th>
														<th>Email</th>
														<th>Education</th>
														<th>Mobile Number</th>
														<th>Join Date</th>
														<th>Birth Date</th>
														<th>Action</th>
													</tr>
												</thead>
												<tbody>
													@foreach(var item in Model.StudentList){
														<tr>
														<td><img class="rounded-circle" width="35" src="images/profile/small/pic2.jpg" alt=""></td>
														<td><strong>@item.FirstName</strong></td>
														<td><strong>@item.LastName</strong></td>
														<td>@item.Email</td>
														<td>@item.Education</td>
														<td><a href="javascript:void(0);"><strong>@item.MobileNumber</strong></a></td>
														<td>@item.BirthDate</td>
														<td>@item.JoinDate</td>
														<td>
															<a asp-action="OgrenciDuzenle" asp-controller="Student" asp-route-studentid="@item.ID" class="btn btn-sm btn-primary"><i class="la la-pencil"></i></a>
															<a asp-action="OgrenciSil" asp-controller="Student" asp-route-studentid="@item.ID" class="btn btn-sm btn-danger"><i class="la la-trash-o"></i></a>
														</td>
													</tr>
													}
											
												</tbody>
